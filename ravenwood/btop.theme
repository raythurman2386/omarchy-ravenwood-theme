# All graphs and meters can be gradients
# For single color graphs leave "mid" and "end" variable empty.
# Use "start" and "end" variables for two color gradient
# Use "start", "mid" and "end" for three color gradient

# Main background, empty for terminal default, need to be empty if you want transparent background
theme[main_bg]="#1a1f1c"

# Main text color
theme[main_fg]="#e8d5b7"

# Title color for boxes
theme[title]="#e8d5b7"

# Highlight color for keyboard shortcuts
theme[hi_fg]="#e67e80"

# Background color of selected items
theme[selected_bg]="#2d3830"

# Foreground color of selected items
theme[selected_fg]="#4ade80"

# Color of inactive/disabled text
theme[inactive_fg]="#3d4a40"

# Color of text appearing on top of graphs, i.e uptime and current network graph scaling
theme[graph_text]="#e8d5b7"

# Misc colors for processes box including mini cpu graphs, details memory graph and details status text
theme[proc_misc]="#4ade80"

# Cpu box outline color
theme[cpu_box]="#2d3830"

# Memory/disks box outline color
theme[mem_box]="#2d3830"

# Net up/down box outline color
theme[net_box]="#2d3830"

# Processes box outline color
theme[proc_box]="#2d3830"

# Box divider line and small boxes line color
theme[div_line]="#2d3830"

# Temperature graph colors
theme[temp_start]="#4ade80"
theme[temp_mid]="#fbbf24"
theme[temp_end]="#e67e80"

# CPU graph colors
theme[cpu_start]="#4ade80"
theme[cpu_mid]="#fbbf24"
theme[cpu_end]="#e67e80"

# Mem/Disk free meter
theme[free_start]="#e67e80"
theme[free_mid]="#fbbf24"
theme[free_end]="#4ade80"

# Mem/Disk cached meter
theme[cached_start]="#22d3ee"
theme[cached_mid]="#34d399"
theme[cached_end]="#4ade80"

# Mem/Disk available meter
theme[available_start]="#e67e80"
theme[available_mid]="#fbbf24"
theme[available_end]="#4ade80"

# Mem/Disk used meter
theme[used_start]="#4ade80"
theme[used_mid]="#fbbf24"
theme[used_end]="#e67e80"

# Download graph colors
theme[download_start]="#4ade80"
theme[download_mid]="#34d399"
theme[download_end]="#22d3ee"

# Upload graph colors
theme[upload_start]="#fbbf24"
theme[upload_mid]="#e69875"
theme[upload_end]="#e67e80"

# Process box color gradient for threads, mem and cpu usage
theme[process_start]="#4ade80"
theme[process_mid]="#e67e80"
theme[process_end]="#da6362"
